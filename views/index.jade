extends layout

block content
    div.ib.info
        h1= title
        p#prompt #{title} is loading...
        img(src=qrUrl, alt='Session Code')
    pre#msgBox.ib
    script(src='/bower_components/socket.io-client/socket.io.js')
    script(src='/bower_components/jquery/dist/jquery.min.js')
    script.
        window.scpSessionId = '#{sessionId}';
        window.scpChannel = '#{channelId}';
    script(src = '/javascripts/script.js')
